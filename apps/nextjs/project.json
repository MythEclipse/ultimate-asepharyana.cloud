{
  "name": "@asepharyana/nextjs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nextjs",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["nextjs-e2e"],
  "// targets": "to see all targets run: nx show project nextjs --web",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "@asepharyana/nextjs:build",
        "dev": true,
        "port": 4090
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/nextjs/"
      },
      "dependsOn": ["services:build"]
    }
  }
}
