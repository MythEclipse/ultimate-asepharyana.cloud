{
  "name": "@asepharyana/nextjs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nextjs",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["nextjs-e2e"],
  "// targets": "to see all targets run: nx show project nextjs --web",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/nextjs && npx next dev --port 4090",
        "cwd": "."
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/nextjs/"
      },
      "dependsOn": ["services:build"]
    }
  }
}
