{
  "name": "@asepharyana/express",
  "version": "1.0.0",
  "description": "Express application for Asepharyana",
  "main": "./dist/cjs/index.js",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "start": "node dist/index.js",
    "build": "bun build src/index.ts --outdir dist",
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "format": "prettier --write .",
    "lint": "concurrently \"eslint .\" \"tsc --noEmit\"",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "update": "bun update"
  },
  "author": "Asepharyana",
  "license": "ISC",
  "devDependencies": {
    "@asepharyana/config-eslint": "workspace:*",
    "@asepharyana/config-typescript": "workspace:*",
    "@asepharyana/database": "workspace:*",
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-love": "^120.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@asepharyana/database": "workspace:*",
    "@eslint/js": "^9.28.0",
    "@types/multer": "^1.4.12",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "multer": "^2.0.1",
    "pdf-lib": "^1.17.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "supertest": "^7.1.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "overrides": {
    "typescript": "^5.8.3"
  }
}