{
  "name": "@asepharyana/express",
  "version": "1.0.0",
  "description": "Express application for Asepharyana",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "start": "node dist/cjs/index.js",
    "build": "npm run build:cjs && npm run build:esm && npm run generate-types",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions \".ts,.tsx\"",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm --extensions \".ts,.tsx\"",
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "format": "prettier --write .",
    "lint": "concurrently \"eslint .\" \"tsc --noEmit\"",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "prebuild": "rimraf dist && npm run generate-types",
    "generate-types": "tsc --emitDeclarationOnly --project tsconfig.json",
    "update": "npm update"
  },
  "author": "Asepharyana",
  "license": "ISC",
  "devDependencies": {
    "@asepharyana/config-eslint": "workspace:*",
    "@asepharyana/config-typescript": "workspace:*",
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-typescript": "~7.26.0",
    "@eslint/eslintrc": "3.x",
    "@jest/globals": "29.x",
    "@types/cors": "2.x",
    "@types/express": "5.x",
    "@types/jest": "~29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/supertest": "6.x",
    "@types/uuid": "10.x",
    "@types/ws": "8.x",
    "@typescript-eslint/eslint-plugin": "8.x",
    "@typescript-eslint/parser": "8.x",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-module-resolver": "5.x",
    "copyfiles": "^2.4.1",
    "cross-env": "7.x",
    "eslint": "9.x",
    "eslint-config-love": "118.x",
    "eslint-config-prettier": "10.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-n": "17.x",
    "eslint-plugin-promise": "7.x",
    "husky": "9.x",
    "jest": "~29.7.0",
    "ncp": "^2.0.0",
    "prettier": "3.x",
    "pretty-quick": "4.x",
    "rimraf": "6.x",
    "ts-jest": "~29.2.5",
    "ts-node": "10.x",
    "tsx": "4.x",
    "typescript": "5.x"
  },
  "dependencies": {
    "@asepharyana/database": "workspace:*",
    "@eslint/js": "9.x",
    "@types/multer": "^1.4.12",
    "body-parser": "1.x",
    "cors": "2.x",
    "dotenv": "16.x",
    "express": "5.x",
    "joi": "17.x",
    "moment": "2.x",
    "multer": "1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pino": "9.x",
    "pino-pretty": "13.x",
    "rate-limiter-flexible": "^5.0.5",
    "sqlite3": "~5.1.7",
    "supertest": "7.x",
    "uuid": "11.x",
    "ws": "8.x",
    "zod": "3.x"
  },
  "overrides": {
    "typescript": "5.x"
  }
}
