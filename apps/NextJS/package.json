{
  "private": true,
  "name": "@asepharyana/web",
  "version": "0.1.0",
  "description": "My personal website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asepharyana71/asepharyana.cloud.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "next dev --port 4090",
    "dev:https": "next dev --experimental-https",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build",
    "postbuild": "next-sitemap",
    "start": "cross-env NODE_ENV=production next start --port 4090",
    "lint": "concurrently \"next lint\" \"tsc --noEmit\"",
    "lint:debug": "next lint  --debug",
    "lint:fix": "next lint --fix",
    "studio": "prisma studio",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-lint": "next lint",
    "update": "npm update"
  },
  "dependencies": {
    "@asepharyana/config-eslint": "workspace:^",
    "@asepharyana/config-typescript": "workspace:*",
    "@asepharyana/database": "workspace:*",
    "@asepharyana/ui": "workspace:*",
    "@auth/prisma-adapter": "^2.7.x",
    "@next/eslint-plugin-next": "^15.1.x",
    "@tabler/icons-react": "3.x",
    "axios": "^1.7.x",
    "cheerio": "^1.0.x",
    "clsx": "^2.1.x",
    "critters": "^0.0.x",
    "date-fns": "4.x",
    "eslint-plugin-react-hooks": "^5.1.x",
    "file-type": "^20.1.x",
    "flowbite-react": "^0.10.x",
    "fluent-ffmpeg": "^2.1.x",
    "form-data": "^4.0.x",
    "framer-motion": "^12.4.x",
    "https-proxy-agent": "^7.0.x",
    "js-yaml": "^4.1.x",
    "lucide-react": "0.456.x",
    "next": "15.1.x",
    "next-auth": "^4.24.x",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.x",
    "node-fetch": "^3.3.x",
    "prisma": "*",
    "react": "^19.0.x",
    "react-dom": "^19.0.x",
    "react-icons": "^5.5.x",
    "react-simplemde-editor": "^5.2.x",
    "reconnecting-websocket": "^4.4.x",
    "sharp": "^0.33.x",
    "swagger-ui-react": "^5.19.x",
    "swr": "^2.3.x",
    "tailwind-merge": "^2.5.x",
    "tailwindcss-animate": "^1.0.x",
    "winston": "^3.17.x",
    "winston-daily-rotate-file": "^5.0.x"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@eslint/eslintrc": "^3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
