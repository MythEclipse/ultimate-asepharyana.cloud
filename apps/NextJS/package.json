{
  "private": true,
  "name": "@asepharyana/web",
  "version": "0.1.0",
  "description": "My personal website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asepharyana71/asepharyana.cloud.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--trace-warnings --inspect' next dev --port 4090 --turbopack",
    "dev:https": "next dev --experimental-https",
    "build": "cross-env next build",
    "postbuild": "next-sitemap",
    "start": "cross-env NODE_ENV=production next start --port 4090",
    "lint": "concurrently \"next lint\" \"tsc --noEmit\"",
    "lint:debug": "next lint  --debug",
    "lint:fix": "next lint --fix",
    "studio": "prisma studio",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check-lint": "next lint",
    "update": "npm update",
    "postinstall": "flowbite-react patch"
  },
  "dependencies": {
    "@asepharyana/config-eslint": "workspace:^",
    "@asepharyana/config-typescript": "workspace:*",
    "@asepharyana/database": "workspace:*",
    "@asepharyana/ui": "workspace:*",
    "@auth/prisma-adapter": "^2.x",
    "@next/eslint-plugin-next": "^15.x",
    "@tabler/icons-react": "^3.x",
    "@tailwindcss/postcss": "^4.0.17",
    "autoprefixer": "^10.x",
    "axios": "^1.x",
    "cheerio": "^1.x",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.x",
    "date-fns": "^4.x",
    "eslint-plugin-react-hooks": "^5.x",
    "file-type": "^20.x",
    "flowbite": "3.1.2",
    "flowbite-react": "^0.x",
    "fluent-ffmpeg": "^2.x",
    "form-data": "^4.x",
    "framer-motion": "^12.x",
    "https-proxy-agent": "^7.x",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.485.0",
    "next": "^15.x",
    "next-auth": "5.0.0-beta.25",
    "next-sitemap": "^4.x",
    "next-themes": "^0.x",
    "next-view-transitions": "^0.x",
    "node-fetch": "^3.x",
    "openapi-types": "^12.x",
    "prisma": "^6.x",
    "react": "^19.x",
    "react-dom": "^19.x",
    "react-icons": "^5.x",
    "react-simplemde-editor": "^5.x",
    "reconnecting-websocket": "^4.x",
    "safe-json-stringify": "^1.x",
    "sharp": "^0.x",
    "swagger-ui-react": "^5.x",
    "swr": "^2.x",
    "tailwind-merge": "^3.x",
    "tailwindcss-animate": "^1.x",
    "tw-animate-css": "^1.x",
    "winston": "^3.x",
    "winston-daily-rotate-file": "^5.x"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.x",
    "@eslint/eslintrc": "^3.x",
    "@types/fluent-ffmpeg": "^2.x",
    "@types/js-yaml": "^4.x",
    "@types/next": "^9.x",
    "@types/node": "^22.x",
    "@types/react": "^19.x",
    "@types/react-dom": "^19.x",
    "@types/safe-json-stringify": "^1.x",
    "@types/swagger-ui-react": "^5.x",
    "eslint": "^9.x",
    "eslint-config-next": "^15.x",
    "postcss": "^8.x",
    "tailwindcss": "^4.x",
    "typescript": "^5.x"
  }
}