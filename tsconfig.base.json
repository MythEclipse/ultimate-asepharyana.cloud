{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "preserve",
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "baseUrl": ".",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "@asepharyana/services": ["dist/libs/services/src/index.js"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
