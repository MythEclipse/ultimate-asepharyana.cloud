  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "clean:nm": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-node-modules.ps1 -Yes",
    "clean:deep": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-node-modules.ps1 -IncludeCache -IncludeLock -PruneStore -Yes",
    "nextjs:dev": "turbo run dev --filter=@asepharyana/nextjs",
    "nextjs:build": "turbo run build --filter=@asepharyana/nextjs",
    "nextjs:lint": "turbo run lint --filter=@asepharyana/nextjs",
    "nextjs:start": "cd apps/nextjs && bun run start",
    "elysia:dev": "turbo run dev --filter=@asepharyana/elysia",
    "elysia:build": "turbo run build --filter=@asepharyana/elysia",
    "elysia:start": "cd apps/elysia && bun run start",
    "elysia:test": "turbo run test --filter=@asepharyana/elysia",
    "rust:build": "cargo build --release --manifest-path apps/rust/Cargo.toml",
    "rust:dev": "cargo run --manifest-path apps/rust/Cargo.toml",
    "service:build": "turbo run build --filter=@asepharyana/services",
    "copyenv": "node -e \"const fs = require('fs'); const path = require('path'); const rootEnv = path.join(__dirname, '.env'); if (!fs.existsSync(rootEnv)) { console.error('Root .env file not found'); process.exit(1); } const appsDir = path.join(__dirname, 'apps'); fs.readdirSync(appsDir).forEach(dir => { const fullDir = path.join(appsDir, dir); if (fs.statSync(fullDir).isDirectory() && !['node_modules', '.turbo', '.next', 'dist', 'target'].includes(dir)) { const dest = path.join(fullDir, '.env'); fs.copyFileSync(rootEnv, dest); console.log('Copied .env to ' + dest); } });\""
  },
