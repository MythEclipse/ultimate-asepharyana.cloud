{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV","PRODUCTION_URL"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "client.js", "client.cjs", "client.d.ts", "client.d.cts", "seed.js", "seed.cjs", "seed.d.ts", "seed.d.cts"]
    },
    "db:migrate:deploy": {},
    "db:push": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {
      "cache": false
    },
    "test": {},
    "start": {
      "cache": false,
      "outputLogs": "full"
    }
  }
}
