{
  "name": "@asepharyana/services",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/services/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/services",
        "tsConfig": "libs/services/tsconfig.json",
        "main": "libs/services/src/index.ts",
        "assets": ["libs/services/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --project libs/services/tsconfig.lib.json && xcopy /E /I /Y libs\\services\\dist dist\\libs\\services && rmdir /s /q libs\\services\\dist"
      }
    }
  },
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag",
        "fallbackCurrentVersionResolver": "disk"
      }
    }
  }
}
